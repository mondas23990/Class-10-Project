New Class (Lakhisarai_Gaya) -



import java.util.*;
import java.io.*;
import java.lang.Thread;
class Lakhisarai_Gaya
{
    void gaya()throws InterruptedException 
    {
        Scanner sc=new Scanner(System.in);
        System.out.println();
                                System.out.println("*****Lakhisarai - Gaya*****");
                                System.out.println();
                                System.out.println();
                                System.out.println("*****Select a Train*****");
                                System.out.println();
                                System.out.println("1. BJU BDTS SPL #09006");
                                System.out.println("2. UTR SMPRK K EXP #15036");
                                System.out.println("3. MMCT TEJAS RAJ #12952");
                                int num2=sc.nextInt();
                                switch(num2)
                                {
                                    case 1:
                                        System.out.println();
                                        System.out.println("*****Please select a coach type*****");
                                        System.out.println();
