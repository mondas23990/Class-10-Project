import java.io.*;
import java.lang.Thread;
class Jamui_Odisha
{
    void jamui()throws InterruptedException
    {
        Scanner sc=new Scanner(System.in);
        System.out.println();
                                System.out.println("*****Jamui - Odisha*****");
                                System.out.println();
