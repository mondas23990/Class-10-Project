                                                        System.out.println("Enter number of passengers(Max 100):- ");
                                                        int num25=sc.nextInt();
                                                        System.out.println();
                                                        Passenger ob200=new Passenger();
                                                        ob200.passenger();
                                                        System.out.println();
                                                        if(num25<100)
                                                        {
                                                            int num26=num25*460;
                                                            System.out.println("Total Fare:- "+num26);
                                                            System.out.println();
                                                        }
                                                        else
                                                        {
                                                            System.out.println("Maximum passengers limit reached, try again");
                                                            System.out.println();
                                                            Booking ob=new Booking();
                                                            ob.booking();
                                                        }
                                                        System.out.println("Seat number allocated are listed below");
                                                        System.out.println();
                                                    for(int i=0;i<=num25;i++)
                                                    {
                                                        Random r=new Random();
                                                        char s=(char)(r.nextInt(26)+'a');
                                                        Random ramd = new Random();
                                                        int romd = ramd.nextInt(10) + 1;
                                                        System.out.println("Seat number allocated:- "+Character.toUpperCase(s)+(romd));
                                                    }
                                                    System.out.println();
                                                    System.out.println();
                                                    System.out.println("To confirm your seat reservations, please pay the amount mentioned above");
                                                    System.out.println();
                                                    System.out.println("1. Finalize Transaction");
                                                    System.out.println("2. Re-Book Tickets");
                                                    System.out.println("3. Back to Main Menu");
                                                    System.out.println("4. Exit");
                                                    int ch109=sc.nextInt();
                                                    switch(ch109)
                                                    {
                                                        case 1:
                                                            Payment ob127=new Payment();
                                                            ob127.payment();
                                                            System.out.println();
                                                            break;
                                                            
                                                        case 2:
                                                            Booking ob128=new Booking();
                                                            ob128.booking();
                                                            System.out.println();
                                                            break;
                                                            
                                                        case 3:
                                                            Railway_Reservation_Software ob129=new Railway_Reservation_Software();
                                                            ob129.software();
                                                            System.out.println();
                                                            break;
                                                        }
                                                    break;
                                                    
                                                    case 2:
                                                        System.out.println();
                                                        Booking ob27=new Booking();
                                                        ob27.booking();
                                                        break;
                                                    
                                                    case 3:
                                                        System.out.println();
                                                        Railway_Reservation_Software ob28=new Railway_Reservation_Software();
                                                        ob28.software();
                                                        break;
                                                    
                                                    default:
                                                        System.out.println("Invalid Number entered, please Try Again ");
                                                        System.out.println();
                                                        Booking ob29=new Booking();
                                                        ob29.booking();
                }
                break;
                
                case 10:
                    
